            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintHorizontal_bias="0.24" app:layout_constraintVertical_bias="0.163"/>
    <TextView
            android:text="Bank Station"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="16dp" android:id="@+id/textView3" android:layout_marginBottom="28dp"
            app:layout_constraintBottom_toTopOf="@+id/textView6" tools:ignore="MissingConstraints"/>
    <TextView
            android:text="Cyprus DLR Station"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView7" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView11" app:layout_constraintEnd_toEndOf="parent"
